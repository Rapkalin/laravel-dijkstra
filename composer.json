{
  "name": "noweh/laravel-dijkstra",
  "description": "Shortest path-algorithm for Laravel.",
  "keywords": [
    "laravel",
    "dijkstra",
    "graph"
  ],
  "require": {
    "ext-bcmath": "*",
    "php": "^7.2 || ^8.0",
    "laravel/framework": "^9.0 || >=8.61.0"
  },
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Julien Schmitt",
      "email": "jschmitt95@protonmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Noweh\\Dijkstra\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Noweh\\Dijkstra\\DijkstraServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
